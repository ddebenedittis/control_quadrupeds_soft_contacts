<library path="rviz_legged_plugins">

    <class
        name="rviz_legged_plugins/ExternalWrench"
        type="rviz_legged_plugins::displays::ExternalWrenchDisplay"
        base_class_type="rviz_common::Display"
    >
        <description>
            Displays an axis with the arrow at the Target Frame's origin.
            &lt;a href="http://www.ros.org/wiki/rviz/DisplayTypes/Axes"&gt;More Information&lt;/a&gt;.
        </description>
    </class>

    <class
        name="rviz_legged_plugins/FrictionCones"
        type="rviz_legged_plugins::displays::FrictionConesDisplay"
        base_class_type="rviz_common::Display"
    >
        <description>
            Display the friction cone inside which the contact forces must lie to satisfy the friction limits.
        </description>
    </class>

    <class
        name="rviz_legged_plugins/Paths"
        type="rviz_legged_plugins::displays::PathsDisplay"
        base_class_type="rviz_common::Display"
    >
        <description>
            Display a list of paths.
        </description>
    </class>

</library>